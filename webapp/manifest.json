{
  "_version": "1.49.0",
  "sap.app": {
    "id": "catalogo.recursos.estudo",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "applicationVersion": {
      "version": "1.0.0"
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "rootView": {
      "viewName": "catalogo.recursos.estudo.view.App",
      "type": "XML",
      "id": "app"
    },
    "dependencies": {
      "minUI5Version": "1.120.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.ui.layout": {},
        "sap.uxap": {}
      }
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "catalogo.recursos.estudo.i18n.i18n"
        }
      },
      "": {
        "type": "sap.ui.model.json.JSONModel",
        "dataSource": "recursos",
        "settings": {
          "defaultBindingMode": "TwoWay"
        }
      }
    },
    "dataSources": {
      "recursos": {
        "type": "JSON",
        "uri": "model/Resources.json"
      }
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "viewPath": "catalogo.recursos.estudo.view",
        "controlId": "app",
        "controlAggregation": "pages",
        "transition": "slide",
        "bypassed": {
          "target": "notFound"
        },
        "async": true
      },
      "routes": [
        {
          "pattern": "",
          "name": "lista",
          "target": "lista"
        },
        {
          "pattern": "detalhe/{disciplina}/{recursoIndex}",
          "name": "detalhe",
          "target": "detalhe"
        }
      ],
      "targets": {
        "lista": {
          "viewName": "Lista",
          "viewLevel": 1,
          "viewId": "lista"
        },
        "detalhe": {
          "viewName": "Detalhe",
          "viewLevel": 2,
          "viewId": "detalhe"
        },
        "notFound": {
          "viewName": "NotFound",
          "viewLevel": 3,
          "viewId": "notFound"
        }
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    }
  }
}