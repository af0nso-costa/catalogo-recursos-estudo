<mvc:View
    controllerName="catalogo.recursos.estudo.controller.NotFound"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true">
    <MessagePage
        id="notFoundPage"
        title="{i18n>notFoundTitle}"
        text="{i18n>notFoundText}"
        description=""
        icon="sap-icon://document"
        showNavButton="true"
        navButtonPress=".onNavBack">
        <customDescription>
            <Link
                text="{i18n>voltarInicio}"
                press=".onNavBack"/>
        </customDescription>
    </MessagePage>
</mvc:View>